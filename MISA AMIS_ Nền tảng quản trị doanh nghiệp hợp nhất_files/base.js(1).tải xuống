function getRandomIntBetween(min,max){min=Math.ceil(min);max=Math.floor(max);return Math.floor(Math.random()*(max-min+1))+min;}
function countDown(i,callback){var timerRandom=setInterval(function(){document.getElementById("getpassCode").innerHTML=i;i--||(clearInterval(timerRandom),callback());},1000);}}}
